{
    "sdk": "gradio",
    "sdk_version": "4.44.0",
    "python_version": "3.11",
    "app_file": "app.py",
    "app_port": 7860,
    "runtime": "python3.11",
    "resources": {
        "cpu": 1,
        "memory": "16Gi",
        "gpu": 0,
